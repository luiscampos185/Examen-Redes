enable
configure terminal

hostname Multilayer-Zona-Azul
no service password-encryption

ip routing
no ip cef
no ipv6 cef
username ADMIN privilege 15 password 0 123

ip ssh version 2
ip domain-name JuanMark.com
spanning-tree mode pvst

ip arp inspection vlan 33-37
ip dhcp snooping vlan 33-37
no ip dhcp snooping information option
ip dhcp snooping
ip flow-export version 9

vlan 33
vlan 34
vlan 35
vlan 36
vlan 37
exit

interface range GigabitEthernet1/0/1-19
switchport mode access
switchport port-security
switchport port-security mac-address sticky
exit

interface range GigabitEthernet1/0/20-24
switchport trunk native vlan 34
switchport trunk allowed vlan 33-37
switchport mode trunk
switchport nonegotiate
ip dhcp snooping trust
ip arp inspection trust
exit

interface range GigabitEthernet1/0/20-24
switchport trunk native vlan 34
switchport trunk allowed vlan 33-37
switchport mode trunk
switchport nonegotiate
ip dhcp snooping trust
ip arp inspection trust
exit

interface Vlan1
no ip address
shutdown
exit

interface Vlan34
ip address 148.60.34.8 255.255.255.0
no shutdown
exit

interface Vlan33
no ip address
exit
interface Vlan35
no ip address
exit
interface Vlan36
no ip address
exit
interface Vlan37
no ip address
exit

ip default-gateway 148.60.34.1

line vty 0 4
login local
transport input ssh
exit

