enable
configure terminal
hostname Router_Centro

no service password-encryption
no ip cef
no ipv6 cef
username ADMIN privilege 15 password 0 123

ip ssh version 2
ip domain-name JuanMark.com
ip flow-export version 9


ip dhcp excluded-address 148.60.33.1 148.60.33.10
ip dhcp excluded-address 148.60.34.1 148.60.34.10
ip dhcp excluded-address 148.60.35.1 148.60.35.10
ip dhcp excluded-address 148.60.36.1 148.60.36.10
ip dhcp excluded-address 148.60.37.1 148.60.37.10

ip dhcp pool POOL_GestionTI
network 148.60.33.0 255.255.255.0
default-router 148.60.33.1
exit

ip dhcp pool POOL_GestionWLAN
network 148.60.34.0 255.255.255.0
default-router 148.60.34.1
exit

ip dhcp pool POOL_VentasWLAN
network 148.60.35.0 255.255.255.0
default-router 148.60.35.1
exit

ip dhcp pool POOL_Compras
network 148.60.36.0 255.255.255.0
default-router 148.60.36.1
exit

ip dhcp pool POOL_Practicantes
network 148.60.37.0 255.255.255.0
default-router 148.60.37.1
exit

interface GigabitEthernet0/0
no ip address
duplex auto
speed auto
no shutdown
exit

interface GigabitEthernet0/0.33
encapsulation dot1Q 33
ip address 148.60.33.1 255.255.255.0
exit

interface GigabitEthernet0/0.34
encapsulation dot1Q 34 native
ip address 148.60.34.1 255.255.255.0
exit

interface GigabitEthernet0/0.35
encapsulation dot1Q 35
ip address 148.60.35.1 255.255.255.0
exit

interface GigabitEthernet0/0.36
encapsulation dot1Q 36
ip address 148.60.36.1 255.255.255.0
exit

interface GigabitEthernet0/0.37
encapsulation dot1Q 37
ip address 148.60.37.1 255.255.255.0
exit

interface Serial0/2/0
ip address 148.60.200.18 255.255.255.252
clock rate 2000000
no shutdown
exit

interface Serial0/2/1
ip address 148.60.200.14 255.255.255.252
clock rate 2000000
no shutdown
exit

interface Serial0/3/0
ip address 148.60.200.10 255.255.255.252
clock rate 2000000
no shutdown
exit

router rip
version 2
network 148.60.0.0
no auto-summary
exit

line con 0
exec-timeout 0 0
line vty 0 4
login local
transport input ssh
exit

